                <td>소분류명</td>
                <td>시작일</td>
                <td>종료일</td>
            </thead>

            <tr>
                <td><%= lists[i].main_id%></td>
                <td><%= lists[i].main_name%></td>
                <td><%= lists[i].sub_id%></td>
                <td><%= lists[i].sub_name%></td>
                <td><%= lists[i].start%></td>
                <td><%= lists[i].end%></td>
                <td>수정</td>
                <td>삭제</td>

            </tr>

        </table>
        <% i += 1 } %><% } %>
<% } %>