<!-- 201935231 컴퓨터공학과 김용우 -->
<% if (logined === 'NO') { %>
	<script>
		alert('로그인이 필요합니다.');
		location.href = '/';
	</script>
<% } %>
<% if (logined === 'USER') { %>
	<script>
		alert('권한이 부족합니다.');
		location.href = '/';
	</script>
<% } %>
<% if (act === 'v') { %>
    <h3>코드 조회</h3>
    <% if (lists.length === 0) { %>
        <table class="table table-bordered border-primary ">
            <tr>
                <td>내용 없음</td>
            </tr>
        </table>
    <% } else { %>
        <% var i = 0;
        while(i < lists.length) { %>
            <table class="table table-bordered border-primary">
                <thead>
                    <td>대분류</td>
                    <td>대분류명</td>
                    <td>소분류</td>
                    <td>소분류명</td>
                    <td>시작일</td>
                    <td>종료일</td>
                </thead>
                <tr>
                    <td><%= lists[i].main_id %></td>
                    <td><%= lists[i].main_name %></td>
                    <td><%= lists[i].sub_id %></td>
                    <td><%= lists[i].sub_name %></td>
                    <td><%= lists[i].start %></td>
                    <td><%= lists[i].end %></td>
                </tr>
            </table>
            <% i += 1 } %><% } %>
<% } else { %>
    <h3>코드 수정</h3>
<% if (lists.length === 0) { %>
    <table class="table table-bordered border-primary ">
        <tr>
            <td>내용 없음</td>
        </tr>
    </table>
<% } else { %>
    <% var i = 0;
    while(i < lists.length) { %>
        <table class="table table-bordered border-primary">
            <thead>
                <td>대분류</td>
                <td>대분류명</td>
                <td>소분류</td>
                <td>소분류명</td>
                <td>시작일</td>
                <td>종료일</td>
            </thead>

            <tr>
                <td><%= lists[i].main_id%></td>
                <td><%= lists[i].main_name%></td>
                <td><%= lists[i].sub_id%></td>
                <td><%= lists[i].sub_name%></td>
                <td><%= lists[i].start%></td>
                <td><%= lists[i].end%></td>
                <td><a href="/code/update/<%= lists[i].main_id %>/<%= lists[i].sub_id %>">수정</a></td>
                <td><a href="/code/delete/<%= lists[i].main_id %>/<%= lists[i].sub_id %>" onclick='if(confirm("정말로 삭제하시겠습니까?")==false){ return false }' >삭제</a></td>
            </tr>
        </table>
        <% i += 1 } %><% } %>
<% } %>